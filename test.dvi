<macro>
    # this is a comment #
    # data types: #
    
    $template = { $a, $b } -> {
        <label>
            --position: ($a, $b);
        </label>
    };

</macro>
<canvas>
    <panel>
        --position: # this is a vector # (20%, 30%);
        --width: (10);
        --height: (10);

        <label>
            --position: (0, 0);
            --width: (10);
            --height: (10);
            --text: "Hello测试";
        </label>
    
    </panel>
</canvas>