<var>
    # this is a comment #
    # data types: #
    $my_font: @ft = `mono`;
    $my_font_2: @ft = `sans serif`;
    $content: @str = "yeah";
    $varvec: @vec = (10, 20, 30);
    $integer: @vec = (10);
    $decimal: @vec = (1.23232);
    $percent: @vec = (30%);
    
    $template: @exp = { $a: @str, $b: @vec } -> {
        <label>
            --position: ($b, $b);
        </label>
    };
</var>
<canvas>
    <panel>
        --position: # this is a vector # (20, 30);
        --width: (10);
        --height: (10);

        <label>
            --position: (0, 0);
            --width: (10);
            --height: (10);
            --text: "Hello测试";
        </label>
    
    </panel>
</canvas>