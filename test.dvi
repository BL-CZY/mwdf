<var>
    # this is a comment #
    # data types: #
    $my_font: @ft = `mono`;
    $my_font_2: @ft = `sans serif`;
    $content: @str = "yeah";
    $varvec: @vec = (10, 20, 30);
    $integer: @vec = (10);
    $decimal: @vec = (1.23232);
    $percent: @vec = (30%);
    
    $template: @exp = { $a: @str, $b: @vec } -> {
        <label>
        # this is a mark #
        *property | @inherit(parent):
            ^position: @copy(parent) + ($b, $b);
        </label>
    };
</var>
<canvas>
    <panel>

    *property:
        ^position: # this is a vector # (20, 30);
        ^size: (10, 10);

    *tag:
        <label>
        *property | @inherit(parent):
            ^position: @inherit(parent);
            ^size: (10%, 10%);
            ^text: "Hello" + $my_font + $content + "World!测试";
        </label>
    
    </panel>
</canvas>